<!-- Registration Section -->
    <div class="bg-light py-3 py-md-5">
        <div class="container">
            <div class="row justify-content-md-center">
                <div class="col-12 col-md-11 col-lg-8 col-xl-7 col-xxl-6">
                    <div class="bg-white p-4 p-md-5 rounded shadow-sm">
                        <!-- Heading -->
                        <div class="row">
                            <div class="col-12">
                                <div class="mb-5">
                                    <h2 class="h3">@ViewData["Title"]</h2>
                                    <h3 class="fs-6 fw-normal text-secondary m-0">Enter your details to register</h3>
                                </div>
                            </div>
                        </div>

                        <!-- Registration Form -->
                        <form id="registerForm" asp-route-returnUrl="@Model.ReturnUrl" method="post">
                            <!-- Validation Summary -->
                            <div asp-validation-summary="ModelOnly" class="text-danger" role="alert"></div>

                            <div class="row gy-3 gy-md-4 overflow-hidden">
                                <!-- Email -->
                                <div class="col-12">
                                    <label asp-for="Input.Email" class="form-label">Email <span
                                            class="text-danger">*</span></label>
                                    <input asp-for="Input.Email" class="form-control" placeholder="name@example.com"
                                        required />
                                    <span asp-validation-for="Input.Email" class="text-danger"></span>
                                </div>

                                <!-- Password -->
                                <div class="col-12">
                                    <label asp-for="Input.Password" class="form-label">Password <span
                                            class="text-danger">*</span></label>
                                    <input asp-for="Input.Password" class="form-control" placeholder="Your Password"
                                        required />
                                    <span asp-validation-for="Input.Password" class="text-danger"></span>
                                </div>

                                <!-- Confirm Password -->
                                <div class="col-12">
                                    <label asp-for="Input.ConfirmPassword" class="form-label">Confirm Password <span
                                            class="text-danger">*</span></label>
                                    <input asp-for="Input.ConfirmPassword" class="form-control"
                                        placeholder="Confirm Password" required />
                                    <span asp-validation-for="Input.ConfirmPassword" class="text-danger"></span>
                                </div>

                                <!-- Terms & Conditions (sample checkbox) -->
                                <div class="col-12">
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" id="iAgree" required />
                                        <label class="form-check-label text-secondary" for="iAgree">
                                            I agree to the <a href="#!" class="link-primary text-decoration-none">terms
                                                and conditions</a>
                                        </label>
                                    </div>
                                </div>

                                <!-- Register Button -->
                                <div class="col-12">
                                    <div class="d-grid">
                                        <button id="registerSubmit" type="submit"
                                            class="btn btn-lg btn-primary">Register</button>
                                    </div>
                                </div>
                            </div>
                        </form>

                        <!-- Already have an account? -->
                        <div class="row">
                            <div class="col-12">
                                <hr class="mt-5 mb-4 border-secondary-subtle" />
                                <p class="m-0 text-secondary text-center">
                                    Already have an account?
                                    <a asp-page="/Account/Login" asp-route-returnUrl="@Model.ReturnUrl"
                                        class="link-primary text-decoration-none">Sign in</a>
                                </p>
                            </div>
                        </div>

                        <!-- External Logins -->
                        <div class="row mt-5">
                            <div class="col-12">
                                <p class="mb-4">Or sign in with</p>

                                @if ((Model.ExternalLogins?.Count ?? 0) == 0)
                                {
                                    <div>
                                        <p>
                                            There are no external authentication services configured. See this
                                            <a href="https://go.microsoft.com/fwlink/?LinkID=532715">article about setting
                                                up this ASP.NET application to support logging in via external services</a>.
                                        </p>
                                    </div>
                                }
                                else
                                {
                                    <form id="external-account" asp-page="./ExternalLogin"
                                        asp-route-returnUrl="@Model.ReturnUrl" method="post" class="form-horizontal">
                                        <div>
                                            <p>
                                                @foreach (var provider in Model.ExternalLogins!)
                                                {
                                                    <button type="submit" class="btn btn-primary me-2" name="provider"
                                                        value="@provider.Name" title="Log in using @provider.DisplayName">
                                                        @provider.DisplayName
                                                    </button>
                                                }
                                            </p>
                                        </div>
                                    </form>
                                }
                            </div>
                        </div>
                        <!-- End External Logins -->

                    </div> <!-- /bg-white card -->
                </div> <!-- /col -->
            </div> <!-- /row -->
        </div> <!-- /container -->
    </div> <!-- /bg-light registration section -->

